@model List<CraftTech.WebUI.Areas.Admin.Models.About.ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Layout/_AdminLayout.cshtml";
}
<br />
<h1>Hakkımızda</h1>
<br />
<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>Açıklama</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutUsID</th>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td><a href="/Admin/AboutUs/UpdateAbout/@item.AboutUsID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
        </tr>
    }
</table>